Cell[BoxData[
 RowBox[{"With", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{
     RowBox[{"blks", "=", 
      RowBox[{"breakSPCBlock", "[", "exampleMSSMmodPath", "]"}]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"inptblk", "=", 
      RowBox[{"createBlockArray", "[", 
       RowBox[{
        RowBox[{"breakSPCBlock", "[", "exampleModPath", "]"}], "[", 

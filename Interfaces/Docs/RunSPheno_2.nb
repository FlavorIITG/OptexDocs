      RowBox[{"createBlockArray", "[", 
       RowBox[{"blks", "[", "\"\<MODSEL\>\"", "]"}], "]"}]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"AddBlocks", "\[Rule]", 
      RowBox[{"SPhenoExtraBlocks", "[", "blks", "]"}]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"SPhenoInputList", "->", 
      RowBox[{"createBlockArray", "[", 
       RowBox[{"blks", "[", "\"\<SPhenoInput\>\"", "]"}], "]"}]}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"SMInputList", "->", "inptblk"}]}], "\[IndentingNewLine]", 
